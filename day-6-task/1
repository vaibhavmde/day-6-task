if (!lock && " " || undefined )
{
  message = "Go away";
}
else
{
 message = "welcome";
}
console.log(message);

//Question 14
Change the code to print

3

2

1

fix.html

<!DOCTYPE html>
<html>
<body>
 <script src=”script.js”></script>
</body>
</html>

script.js

//You can change only 2 characterslet i = 3;while (i) {
  console.log( --i );
}


Answer
let i = 3;
while (i>0) {
  console.log( i );
  i--;
}

Question 15
Change the code to print 1 to 10 in 4 lines

fix.html

<!DOCTYPE html>
<html>
<body>
 <script src=”script.js”></script>
</body>
</html>

script.js

let num = 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)
num += 1
console.log(num)

Answer
for(let i=1;i<=10;i++)
{console.log(i);}

//Question 16
Change the code to print even numbers

fix.html

<!DOCTYPE html>
<html>
<body>
 <script src=”script.js”></script>
</body>
</html>

script.js

//You are allowed to modify only one character for (let num = 2; num <= 20; num += 1) {
  console.log(num)
}

Answer
for (let num = 2; num <= 20; num += 2) {
  console.log(num)
}

//Question 17
Change the code to print all the gifts

fix.html

<!DOCTYPE html>
<html>
<body>
 <script src=”script.js”></script>
</body>
</html>

script.js

let gifts = ["teddy bear", "drone", "doll"];for (let i = 0; i < 3; i++) {
  console.log('Wrapped ${'gifts[i]'} and added a bow!');
}

Answer
let gifts = ["teddy bear", "drone", "doll"];
for (let i = 0; i < 3; i++) {
  console.log(`Wrapped ${gifts[i]} and added a bow!`);
}

//Question 18
Fix the code to disarm the bomb.

fix.html

<!DOCTYPE html>
<html>
<body>
 <script src=”script.js”></script>
</body>
</html>

script.js

let countdown = 100;while (countdown > 0) {
  countdown--;
  if(countdown == 0)
  {
   console.log("bomb triggered");
  }
}

Answer
let countdown = 100;
while (countdown > 0) {
  countdown--;
  if(countdown == 0)
  {
   console.log("bomb triggered");
  }else{console.log("disarm the bomb");}
  break;
}

//Question 19
Whats the msg printed and why?

var lemein = “0”;
var lemeout = 0;
var msg = “”;if (lemein) {
 msg += “hi”;
 }if (lemeout) {
 msg += ‘Hello’;
}console.log(msg);

Answer
// print hi if(0) is false, if("0") is true thats why it print hi

